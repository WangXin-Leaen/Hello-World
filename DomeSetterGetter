public class Dome{
  private String name;
  private int age;
  public void setNmane(String name){
    name = this.name;
  }
  public void setAge(int age){
    age = this.age;
  }
  public String getName(){
    return name;  
  }
  public int getAge(){
    return age;
  }
  public void info(){
    System.out.println("name: " + name +"\n" + "age: " + age );
    
  }
}
